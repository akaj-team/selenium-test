<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Suite" thread-count="2" parallel="tests" data-provider-thread-count="2">

    <test name="FireFox Test">
        <parameter name="browserName" value="firefox"/>
        <parameter name="server" value="http://192.168.99.1:4444/wd/hub"/>
        <classes>
            <class name="TestRunnerFirefox"/>
        </classes>
    </test>

    <test name="Chrome Test">
        <parameter name="browserName" value="chrome"/>
        <parameter name="server" value="http://192.168.99.1:4444/wd/hub"/>
        <classes>
            <class name="TestRunnerChrome"/>
        </classes>
    </test>
</suite>
